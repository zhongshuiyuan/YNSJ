<?xml version="1.0" encoding="UTF-8"?>
<project name="module_swipemenulistview" default="compile.module.swipemenulistview">
  <dirname property="module.swipemenulistview.basedir" file="${ant.file.module_swipemenulistview}"/>
  
  <property name="module.jdk.home.swipemenulistview" value="${jdk.home.android_api_19_platform}"/>
  <property name="module.jdk.bin.swipemenulistview" value="${jdk.bin.android_api_19_platform}"/>
  <property name="module.jdk.classpath.swipemenulistview" value="jdk.classpath.android_api_19_platform"/>
  
  <property name="compiler.args.swipemenulistview" value="-encoding UTF-8 -source 1.6 -target 1.6 ${compiler.args}"/>
  
  <property name="swipemenulistview.output.dir" value="${module.swipemenulistview.basedir}/build/intermediates/classes/debug"/>
  <property name="swipemenulistview.testoutput.dir" value="${module.swipemenulistview.basedir}/build/intermediates/classes/test/debug"/>
  
  <path id="swipemenulistview.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="swipemenulistview.module.production.classpath">
    <path refid="${module.jdk.classpath.swipemenulistview}"/>
  </path>
  
  <path id="swipemenulistview.runtime.production.module.classpath">
    <pathelement location="${swipemenulistview.output.dir}"/>
  </path>
  
  <path id="swipemenulistview.module.classpath">
    <path refid="${module.jdk.classpath.swipemenulistview}"/>
    <pathelement location="${swipemenulistview.output.dir}"/>
  </path>
  
  <path id="swipemenulistview.runtime.module.classpath">
    <pathelement location="${swipemenulistview.testoutput.dir}"/>
    <pathelement location="${swipemenulistview.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.swipemenulistview">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.swipemenulistview">
    <patternset refid="excluded.from.module.swipemenulistview"/>
  </patternset>
  
  <path id="swipemenulistview.module.sourcepath">
    <dirset dir="${module.swipemenulistview.basedir}">
      <include name="build/generated/source/r/debug"/>
      <include name="build/generated/source/aidl/debug"/>
      <include name="build/generated/source/buildConfig/debug"/>
      <include name="build/generated/source/rs/debug"/>
      <include name="build/generated/res/rs/debug"/>
      <include name="build/generated/res/resValues/debug"/>
      <include name="res"/>
      <include name="src"/>
      <include name="assets"/>
      <include name="src"/>
    </dirset>
  </path>
  
  <path id="swipemenulistview.module.test.sourcepath">
    <dirset dir="${module.swipemenulistview.basedir}">
      <include name="build/generated/source/r/androidTest/debug"/>
      <include name="build/generated/source/aidl/androidTest/debug"/>
      <include name="build/generated/source/buildConfig/androidTest/debug"/>
      <include name="build/generated/source/rs/androidTest/debug"/>
      <include name="build/generated/res/rs/androidTest/debug"/>
      <include name="build/generated/res/resValues/androidTest/debug"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.swipemenulistview" depends="compile.module.swipemenulistview.production,compile.module.swipemenulistview.tests" description="Compile module SwipeMenuListView"/>
  
  <target name="compile.module.swipemenulistview.production" depends="register.custom.compilers" description="Compile module SwipeMenuListView; production classes">
    <mkdir dir="${swipemenulistview.output.dir}"/>
    <javac2 destdir="${swipemenulistview.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.swipemenulistview}/javac">
      <compilerarg line="${compiler.args.swipemenulistview}"/>
      <bootclasspath refid="swipemenulistview.module.bootclasspath"/>
      <classpath refid="swipemenulistview.module.production.classpath"/>
      <src refid="swipemenulistview.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.swipemenulistview"/>
    </javac2>
    
    <copy todir="${swipemenulistview.output.dir}">
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/r/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/aidl/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/buildConfig/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/res/rs/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/res/resValues/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/res">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/assets">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.swipemenulistview.tests" depends="register.custom.compilers,compile.module.swipemenulistview.production" description="compile module SwipeMenuListView; test classes" unless="skip.tests">
    <mkdir dir="${swipemenulistview.testoutput.dir}"/>
    <javac2 destdir="${swipemenulistview.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.swipemenulistview}/javac">
      <compilerarg line="${compiler.args.swipemenulistview}"/>
      <bootclasspath refid="swipemenulistview.module.bootclasspath"/>
      <classpath refid="swipemenulistview.module.classpath"/>
      <src refid="swipemenulistview.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.swipemenulistview"/>
    </javac2>
    
    <copy todir="${swipemenulistview.testoutput.dir}">
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/r/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/aidl/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/buildConfig/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/source/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/res/rs/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.swipemenulistview.basedir}/build/generated/res/resValues/androidTest/debug">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.swipemenulistview" description="cleanup module">
    <delete dir="${swipemenulistview.output.dir}"/>
    <delete dir="${swipemenulistview.testoutput.dir}"/>
  </target>
</project>